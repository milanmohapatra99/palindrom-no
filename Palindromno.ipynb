   "source": [
    "#w.a.p to input no check wheather this no is palindrome or not\n",
    "n  = int(input(\"Enter No:-\"))\n",
    "n1 = n\n",
    "rev =0 \n",
    "while(n != 0):\n",
    "    dg = int(n % 10)\n",
    "    rev = rev * 10 +dg\n",
    "    n = n // 10\n",
    "      \n",
    "if (n1 == rev):\n",
    "    print('no is palindrom')\n",
    "else:\n",
    "    print('no is not palindrom')"
   ]
